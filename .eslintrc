{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", {
            "semi": true,
            "tabWidth": 4,
            "singleQuote": true,
            "trailingComma": "es5",
            "printWidth": 100,
            "bracketSpacing": true,
            "endOfLine": "auto"
        }]
    }
}